{
    "debug.console.fontSize": 18,
    "explorer.confirmDelete": false,
    "editor.multiCursorModifier": "alt",
    "git.allowForcePush": true,
    "git.confirmForcePush": false,
    "python.languageServer": "Pylance",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.inlineSuggest.enabled": true,
    "remote.SSH.remotePlatform": {
        "sandbox.pintql.csb": "linux"
    },
    "svelte.enable-ts-plugin": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "svelte.plugin.svelte.defaultScriptLanguage": "ts",
    "svelte.plugin.svelte.format.config.singleQuote": true,
    "javascript.format.semicolons": "remove",
    "javascript.preferences.quoteStyle": "single",
    "prettier.documentSelectors": [
        "*.svelte"
    ],
    "svelte.plugin.svelte.format.config.printWidth": 100,
    "[python]": {
        "editor.formatOnType": true
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false
    },
    "window.zoomLevel": 2,
    "chat.agent.enabled": true,
    "git.autofetch": true,
    "makefile.configureOnOpen": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "vim.easymotion": true,
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.quickOpen"
    ],
    "terminal.integrated.env.linux": {

    },
    "vim.leader": ",",
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    },
    "accessibility.voice.speechLanguage": "ru-RU",
    "github.copilot.chat.codesearch.enabled": true,
    "github.copilot.chat.agent.thinkingTool": true,
    "github.copilot.chat.editor.temporalContext.enabled": true,
    "github.copilot.chat.edits.temporalContext.enabled": true,
    "github.copilot.chat.generateTests.codeLens": true,
    "yaml.schemas": {
        "file:///home/kpi/.vscode/extensions/atlassian.atlascode-3.8.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "redhat.telemetry.enabled": false,
}